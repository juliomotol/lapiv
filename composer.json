{
    "name": "juliomotol/lapiv",
    "description": "API Versioning Package for Laravel 6+",
    "keywords": [
        "juliomotol",
        "lapiv",
        "api",
        "versioning"
    ],
    "homepage": "https://github.com/juliomotol/lapiv",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Julio Motol",
            "email": "julio.motol89@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/http": "^6.0|^7.0|^8.0",
        "illuminate/routing": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "JulioMotol\\Lapiv\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JulioMotol\\Lapiv\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JulioMotol\\Lapiv\\LapivServiceProvider"
            ],
            "aliases": {
                "Lapiv": "JulioMotol\\Lapiv\\LapivFacade"
            }
        }
    }
}
